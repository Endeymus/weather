<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.endeymus.weather.models">

    <class name="CityTB" table="citytb">
        <id name="name" type="java.lang.String" column="name"/>
        <property name="temperature" type="double" column="temperature"/>
        <property name="minutes" type="long" column="minutes"/>
    </class>

</hibernate-mapping>